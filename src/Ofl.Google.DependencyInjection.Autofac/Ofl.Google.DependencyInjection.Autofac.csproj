<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Ofl.Google.Contract implementation registrations in Autofac.</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Ofl.Google.DependencyInjection.Autofac</AssemblyName>
    <PackageId>Ofl.Google.DependencyInjection.Autofac</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.Google.DependencyInjection.Autofac</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.Google.DependencyInjection.Autofac.git</RepositoryUrl>
    <PackageTags>google google-api dependency-injection autofac</PackageTags>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.1.0" />
    <PackageReference Include="Ofl.Google" Version="2.0.3" />
    <PackageReference Include="Ofl.Google.Abstractions" Version="1.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
